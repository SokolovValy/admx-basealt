        <checkBox refId="gnome-weekday_blocker">Блокировать</checkBox>
      </presentation>
      <presentation id="OrgGnomeDesktopInterfaceClockShowWeekdayUser-pr">
        <dropdownList noSort="true" defaultItem="0" refId="gnome-weekday_setter">Отображение дня недели:</dropdownList>
      </presentation>
    </presentationTable>
  </resources>
